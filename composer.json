{
    "name": "timostamm/websocket-server",
    "description": "A simple nonblocking server dedicated to websockets.",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "react/socket": "^1.0",
        "ratchet/rfc6455": "^0.2.4",
        "guzzlehttp/psr7": "^1.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Timo Stamm",
            "email": "ts@timostamm.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "TS\\Websockets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TS\\Websockets\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    }
